{
    "plugins": [
        "prettier"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "eslint-config-prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/newline-after-import": "off",
        "import/no-unresolved": "warn",
        "no-param-reassign": ["error", { "props": false }],
        "class-methods-use-this": "off",
        "no-unused-vars": ["error", { "args": "none" }]
    }
}
